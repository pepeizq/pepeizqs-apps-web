@page
@model pepeizqs_apps_web.Pages.games.pepeizqs_citiesModel

@{
    string id = "cities";

    ViewData["Title"] = Listados.Proyectos.DevolverProyecto(id).Nombre + " • " + Herramientas.Idiomas.CogerCadena(Model.idioma, Listados.Proyectos.DevolverProyecto(id).Id + ".String1");
    Layout = "/Pages/Shared/_Layout.cshtml";

    Modulos.Intro intro = new Modulos.Intro()
    {
        Texto1 = "2"
    };
}

@(await Html.RenderComponentAsync<Componentes.Plantillas.Juego>(RenderMode.ServerPrerendered, new { 
    idioma = Model.idioma,
    id = id,
    intro = intro,
    capturas = 9
}))